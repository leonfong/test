               <div class="form-group  nav navbar-nav navbar-left">
                  
                  <lable for="order" , class="sr-only">Qrder : </lable>
                  <div class="input-group input-group-sm">    
                                        
                      <%= text_field_tag :order_no, params[:order_no], class:"form-control input-lg", size:"20", placeholder:"单号"%>
             
                  <div class="input-group-btn">
                      <%= submit_tag "搜索", class:"btn btn-primary btn-sm" %>
                  </div>
                </div>
                <a type="button" class="btn btn-success btn-sm" href="/factory_in">料齐的订单</a>
            </div>
            <% end %>                       
            </span>
            </div>
        </div>
<br>
<%= render 'layouts/flash_messages' %>


        <div class="row">
            <div class="col-md-12 col-sm-12 form-inline" >
<% if not @buy_done.blank? %>   
<% @buy_done.each do |wh| %>                
<table class="table" style="padding: 0px;margin: 0px;">
