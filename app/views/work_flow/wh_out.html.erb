<div class="container-fluid" >
    <div class="row" style="padding: 0px;">
        <div class="col-md-1 list-group" style="padding: 0px;">
            <%= render partial: "work_flow/menu", locals: {selected: "factory_out"} %>
        </div>
        <div class="col-md-11" style="padding-top: 10px;"><small>
        <% if can? :work_e, :all %>
            <div style="padding: 10px;">
                <a type="button" class="btn btn-primary btn-sm"  href="/new_wh_out">新建发货通知</a>   
            </div>
        <% end %>
<% if not @whlist.blank? %>
<%= will_paginate(@whlist) %>            
<table class="table table-hover">
  <thead>
    <tr style="background-color: #eeeeee">
      <th width="220">发货单号</th>
      <th width="220">状态</th>
      <th >发货申请</th>
      <th width="150">日期</th>
<% if can? :work_wh, :all %>
      <th width="50">删除</th>
<% end %>
    </tr>
  </thead>
  <tbody><small>
    <% @whlist.each do |wh| %>   
      <tr >
        <td><%= link_to wh.wh_out_no, edit_wh_out_path(wh_out_no: wh.wh_out_no) %></td>
        <td><%= wh.state %></td>
        <td><%= User.find_by(email: wh.wh_out_user).full_name %></td>
        <td><%= wh.created_at.localtime.strftime('%Y-%m-%d %H:%M:%S').to_s %></td>
<% if can? :work_wh, :all %>
        <td><div class="btn-group"><a class="btn btn-danger btn-xs" data-method='get'  href="/del_wh?wh_id=<%= wh.id.to_s %>" data-confirm="确定要删除?">删除</a></div></td>
<% end %>

      </tr>
    <% end %>
  </small>
  </tbody>
</table>
<% end %>

       </small></div>
    </div>








</div>

<script>
$(".menu-first").click(function(){
  $('#userMeun').collapse('hide');
  $('#articleMenu').collapse('hide');
});
   
</script>


