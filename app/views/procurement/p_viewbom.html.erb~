<script>
window.actionEvents = {
    'click .btn-d': function (e, value, row, index) {
            $("#id").val(row.id);
            $("#btn-del").click();
            $table.bootstrapTable('remove', {
                field: 'mpn',
                values: [row.mpn]
            });
    }
};
