    $("#bank_user_label").dblclick(function(){
        $("#bank_user_show").hide();
        $("#bank_user_edit").show();
    });
    $("#info_a_label").dblclick(function(){
        $("#info_a_show").hide();
        $("#info_a_edit").show();
    });
    $("#info_b_label").dblclick(function(){
        $("#info_b_show").hide();
        $("#info_b_edit").show();
    });
});
