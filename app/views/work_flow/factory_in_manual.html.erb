                <tr style="background-color: #eeeeee">
                    <th>订单号</th>
                    <th>领料单号</th>
                    <th>状态</th>
                </tr>
            </thead>
            <tbody>
            <% LingLiaoDanInfo.where(pcb_order_no_son: wh.pcb_order_no_son).each do |item| %>
                <tr>
                    <td><%= link_to item.pcb_order_no_son, edit_ling_liao_dan_path(id: item.id) %></td>
                    <td><%= item.id %></td>
                    <td><%= item.ling_liao_state %></td>
