$(document).ready(function(){
    $("#new_customer_d").click(function(){
        $("#new_fu_kuan_shen_qing").modal('toggle');
        $("#find_supplier").modal({
            toggle: true,
            backdrop: "static"
        });
        $("#pay_type").val("d");
        $("#pay_type_show").html("日结");
        $("#date_at_label").hide();
        $("#date_at").hide();
    }); 
    $("#new_customer_m").click(function(){
        $("#new_fu_kuan_shen_qing").modal('toggle');
        $("#find_supplier").modal({
            toggle: true,
            backdrop: "static"
        });
        $("#pay_type").val("m");
        $("#pay_type_show").html("月结");
        $("#table_code_supplier").html("");
        $("#date_at_label").show();
        $("#date_at").show();
    });
});


</script>
