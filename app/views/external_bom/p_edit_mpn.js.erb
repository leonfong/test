document.getElementById('matched').innerHTML='<%= @match_str_nn %>';
document.getElementById('total_price').innerHTML='<%=  number_with_precision(@total_price_nn, precision: 4)%>';



<% if @p_item.product_id !=0 %>
document.getElementById('<%= @p_item.id.to_s + "d" %>').innerHTML='<%= Product.find(@p_item.product_id).description %>';
$("#<%= @p_item.id.to_s + 'd' %>").attr("data-content","<%= Product.find(@p_item.product_id).name %>");             
<% else %>
document.getElementById('<%= @p_item.id.to_s + "d" %>').innerHTML='';
$("#<%= @p_item.id.to_s + 'd' %>").attr("data-content","");
<% end %>

<% if @p_item.cost%>
document.getElementById('<%= @p_item.id.to_s + "p" %>').innerHTML='<%= "￥" + @p_item.cost.to_s %>';
document.getElementById('<%= @p_item.id.to_s + "p_t" %>').innerHTML='<%= "￥" + (@p_item.cost*@boms.qty*@p_item.quantity).to_s %>';
<% else %>
document.getElementById('<%= @p_item.id.to_s + "p" %>').innerHTML='￥';
document.getElementById('<%= @p_item.id.to_s + "p_t" %>').innerHTML='￥';
<% end %>


<% if @p_item.color == "b" %>
document.getElementById('<%= @p_item.id.to_s + "_tr" %>').className="bg-info";
<% elsif @p_item.color == "g" %>
document.getElementById('<%= @p_item.id.to_s + "_tr" %>').className="bg-success";
<% else %>
document.getElementById('<%= @p_item.id.to_s + "_tr" %>').className="bg-danger";
<% end %>
$('#editMpn').modal('hide')


