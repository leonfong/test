$('#remarkitem').on('show.bs.modal', function (event) {
  var button = $(event.relatedTarget) // Button that triggered the modal
  var itempid = button.data('itempid');
  var modal = $(this);
  modal.find($("#itemp_id_remark")).val(itempid);
})
function postform_check() {  	           
      if ($("#pay_type").val() == "" )
      {                    
　　    alert("请选填收款性质！");                    
　　    return false;                
　     }
<% if not @shou_kuan_jin_e.blank?%>
      if ($("#pay_p").val() + $("#shou_kuan_jin_e").val() > <%= @pi_info.t_p %>)
      {                    
　　    alert($("#pay_p").val() + $("#shou_kuan_jin_e").val() + "---保存失败，收款金额总和不能大于PI总金额！！！！---"
+ <%= @pi_info.t_p %>);                    
　　    return false;                
　     }
<% end %>
}


$('#add_ecn').on('show.bs.modal', function (event) {
  var button = $(event.relatedTarget) // Button that triggered the modal
  var moko_part = button.data('moko_part') 
  var moko_des = button.data('moko_des')
  var bom_no = button.data('bom_no')


  var modal = $(this)
  modal.find($("#moko_part")).val(moko_part)
  modal.find($("#moko_des")).html(moko_des)
  modal.find($("#bom_no")).html(bom_no)
})




$(document).ready(function(){
    $("#t_p_label").dblclick(function(){
        $("#t_p_show").hide();
        $("#t_p_edit").show();
    });
});
</script>


